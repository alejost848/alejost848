{
  "rules": {
    "headerMessages": {
      ".read": true
    },
    "tutorials": {
      ".read": true,
      "after-effects": {
        ".indexOn": ["publishedDate", "slug"]
      }
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}
