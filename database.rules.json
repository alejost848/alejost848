{
  "rules": {
    "home": {
      ".read": true,
      "latestTutorials": {
        ".indexOn": "publishedDate"
      },
      "latestWorks": {
        ".indexOn": "publishedDate"
      }
    },
    "tutorials": {
      ".read": true,
      "$sid": {
        "videos": {
          ".indexOn": ["publishedDate", "slug"]
        }
      }
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}
