{
  "rules": {
    ".read": "auth.uid === 'd9JhyoWr4LWRviqzQG7Sdzm0PZo2'",
    ".write": "auth.uid === 'd9JhyoWr4LWRviqzQG7Sdzm0PZo2'",
    "home": {
      ".read": true,
      "latestTutorials": {
        ".indexOn": "publishedDate"
      },
      "latestWorks": {
        ".indexOn": "publishedDate"
      }
    },
    "tutorials": {
      ".read": true,
      "series": {
        "$sid": {
          "videos": {
            ".indexOn": ["publishedDate", "slug"]
          }
        }
      }
    },
    "users": {
      "list": {
        "$uid": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        }
      }
    },
    "works": {
      ".read": true,
      "list":{
        ".indexOn": ["publishedDate", "slug"]
      }
    }
  }
}
