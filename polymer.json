{
  "entrypoint": "index.html",
  "shell": "src/portfolio-app.html",
  "fragments": [
    "src/home-view.html",
    "src/tutorials-view.html",
    "src/works-view.html",
    "src/tutorial-view.html",
    "src/work-view.html",
    "src/about-view.html",
    "src/error-view.html",
    "bower_components/paper-dialog/paper-dialog.html",
    "bower_components/paper-toggle-button/paper-toggle-button.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "firebase-messaging-sw.js",
    "bower_components/app-storage/app-indexeddb-mirror/app-indexeddb-mirror-worker.js",
    "bower_components/app-storage/app-indexeddb-mirror/common-worker-scope.js",
    "bower_components/font-roboto/fonts/roboto/Roboto-Light.ttf",
    "bower_components/font-roboto/fonts/roboto/Roboto-Regular.ttf",
    "bower_components/font-roboto/fonts/roboto/Roboto-Medium.ttf",
    "bower_components/font-roboto/fonts/roboto/Roboto-Bold.ttf"
  ],
  "builds": [
    {
      "preset": "es6-unbundled",
      "js": {"minify": false}
    }
  ],
  "lint": {
    "rules": ["polymer-2"]
  }
}
