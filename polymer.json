{
  "entrypoint": "index.html",
  "shell": "src/portfolio-app.html",
  "fragments": [
    "src/home-view.html",
    "src/tutorials-view.html",
    "src/works-view.html",
    "src/tutorial-view.html",
    "src/about-view.html",
    "src/error-view.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "firebase-messaging-sw.js"
  ],
  "builds": [
    {
      "preset": "es5-bundled",
      "js": {"minify": false}
    },
    {
      "preset": "es6-unbundled",
      "js": {"minify": false}
    }
  ],
  "lint": {
    "rules": ["polymer-2"]
  }
}
