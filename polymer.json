{
  "entrypoint": "index.html",
  "shell": "src/portfolio-app.html",
  "fragments": [
    "src/home-view.html",
    "src/tutorials-view.html",
    "src/projects-view.html",
    "src/tutorial-view.html",
    "src/about-view.html",
    "src/error-view.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "firebase-messaging-sw.js"
  ],
  "builds": [{
    "addServiceWorker": true,
    "insertPrefetchLinks": true,
    "js": {"minify": false},
    "css": {"minify": true},
    "html": {"minify": true}
  }],
  "lint": {
    "rules": ["polymer-2"]
  }
}
